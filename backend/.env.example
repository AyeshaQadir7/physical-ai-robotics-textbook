# Cohere Configuration
COHERE_API_KEY=your_cohere_production_key_here
COHERE_MODEL=embed-english-v3.0
BATCH_SIZE=96
MAX_RETRIES=3
REQUEST_TIMEOUT=60

# Qdrant Cloud Configuration
QDRANT_URL=https://your-cluster.qdrant.io
QDRANT_API_KEY=your_qdrant_api_key_here
COLLECTION_NAME=textbook_embeddings

# Chunking Configuration
CHUNK_SIZE=512
CHUNK_OVERLAP=50

# Pipeline Configuration
CHECKPOINT_FILE=ingestion_checkpoint.json
LOG_LEVEL=INFO

# Target
TEXTBOOK_BASE_URL=https://physical-ai-robotics.vercel.app

# OpenAI Agent Configuration (Spec 3)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-3.5-turbo
QDRANT_COLLECTION_NAME=textbook_embeddings
